<?xml version="1.0"?>
<project
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd"
	xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>com.lexicalscope.symb</groupId>
		<artifactId>svm-parent</artifactId>
		<version>0.0.1-SNAPSHOT</version>
	</parent>
	<artifactId>svm-partition-trace-symb</artifactId>
	<name>svm-partition-trace-symb</name>
	<url>http://maven.apache.org</url>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>
   <build>
      <plugins>
         <plugin>
            <groupId>com.lexicalscope.symb</groupId>
            <artifactId>svm-z3-plugin</artifactId>
            <version>${project.version}</version>
            <executions>
               <execution>
                  <goals>
                     <goal>install-z3</goal>
                  </goals>
               </execution>
            </executions>
         </plugin>
      </plugins>
   </build>
	<dependencies>
		<dependency>
			<groupId>com.lexicalscope.symb</groupId>
			<artifactId>svm-jvm-symb</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.lexicalscope.symb</groupId>
			<artifactId>svm-partition-trace</artifactId>
			<version>${project.version}</version>
		</dependency>
      <dependency>
         <groupId>com.lexicalscope.symb</groupId>
         <artifactId>svm-examples-one</artifactId>
         <version>${project.version}</version>
      </dependency>
      <dependency>
         <groupId>com.lexicalscope.symb</groupId>
         <artifactId>svm-examples-two</artifactId>
         <version>${project.version}</version>
      </dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<scope>test</scope>
		</dependency>
	</dependencies>
</project>
